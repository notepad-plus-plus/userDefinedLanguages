<NotepadPlus>
	<functionList>
		<!-- ================================================================= [ STL ] -->
		<!-- STL 3D Object (ASCII)                                                  -->
		<!-- based on rules https://learnvim.irian.to/vimscript/vimscript_functions -->

		<parser
			id="stl_function"
			displayName="STL"
		>
			<function
				mainExpr="^\h*solid\h+\w+"
			>
				<functionName>
					<nameExpr expr="^\h*\Ksolid\h+\w+" />
				</functionName>
			</function>
		</parser>
	</functionList>
</NotepadPlus>
