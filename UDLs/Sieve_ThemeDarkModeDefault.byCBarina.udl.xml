            <WordsStyle name="FOLDER IN CODE2" fgColor="D4D4D4" bgColor="FFFFFF" colorStyle="1" fontStyle="0" nesting="0" />
            <WordsStyle name="FOLDER IN COMMENT" fgColor="D4D4D4" bgColor="FFFFFF" colorStyle="1" fontStyle="0" nesting="0" />
            <WordsStyle name="DELIMITERS1" fgColor="66CC66" bgColor="FFFFFF" colorStyle="1" fontStyle="0" nesting="0" />
