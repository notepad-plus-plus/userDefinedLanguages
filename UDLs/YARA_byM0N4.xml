            <WordsStyle name="FOLDER IN CODE2" fgColor="000000" bgColor="FF80FF" fontStyle="0" nesting="0" />
            <WordsStyle name="FOLDER IN COMMENT" fgColor="000000" bgColor="FFFFFF" fontStyle="0" nesting="0" />
            <WordsStyle name="DELIMITERS1" fgColor="DC322F" bgColor="FF80FF" colorStyle="1" fontStyle="0" nesting="0" />
            <WordsStyle name="DELIMITERS2" fgColor="6C71C1" bgColor="FFFFFF" colorStyle="1" fontStyle="0" nesting="0" />
